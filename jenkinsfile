pipeline {

    agent any 

    stages {

        stage('checkout') {
                         steps {
                            sh '''
                            sudo --preserve-env=SSH_AUTH_SOCK su
                            Defaults    env_keep+=SSH_AUTH_SOCK
                     git clone    https://github.com/jaintpharsha/maven.git
                         '''
                         }
            


        }
    }
}